{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ]
  },
  "include": [
    "./js/**/*"
  ],
  "types": [
    "cypress-commands",
    "cypress"
  ]
}
