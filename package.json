{
  "name": "@concord-consortium/portal-report",
  "version": "1.7.0-pre.13",
  "description": "",
  "main": "dist/library/portal-report.js",
  "files": [
    "dist/library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/portal-report.git"
  },
  "keywords": [
    "portal",
    "rigse",
    "report"
  ],
  "author": "Pior Janik <janikpiotrek@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/portal-report/issues"
  },
  "homepage": "https://github.com/concord-consortium/portal-report#readme",
  "scripts": {
    "start": "webpack-dev-server",
    "start:secure": "webpack-dev-server --https --inline --hot --content-base dist/ --cert ./LocalhostCertificates/localhost.pem --key ./LocalhostCertificates/localhost.key",
    "start:secure:no-certs": "webpack-dev-server --https --inline --hot --content-base dist/",
    "build": "webpack --mode production",
    "prepublishOnly": "WEBPACK_TARGET=lib webpack --mode production",
    "lint": "eslint 'js/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}' 'cypress/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint --fix 'js/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}' 'cypress/**/*.{js,ts,tsx}'",
    "cypress:test": "cypress run --config video=false,trashAssetsBeforeRuns=false",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:full": "npm run test && npm run cypress:test"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTest.js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "testURL": "https://portal-report.unexisting.url.com",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "testRegex": "(_spec)\\.[tj]sx?$",
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "coverageDirectory": "coverage-jest"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage-cypress"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^2.0.5",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/humps": "^1.1.2",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/md5": "^2.1.33",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "autoprefixer": "^9.5.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
    "cypress": "^4.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^2.0.5",
    "jest": "^25.5.4",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nock": "^10.0.6",
    "nyc": "^14.1.1",
    "postcss-loader": "^3.0.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.2",
    "source-map-support": "^0.5.16",
    "style-loader": "^0.23.1",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "aws-sdk": "^2.451.0",
    "bootstrap": "^3.3.6",
    "chroma-js": "^2.0.3",
    "firebase": "^7.14.3",
    "formik": "^1.5.4",
    "humps": "^2.0.1",
    "iframe-phone": "^1.2.0",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "markdown-to-jsx": "^6.9.4",
    "md5": "^2.2.1",
    "normalizr": "^3.3.0",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-bootstrap": "^0.32.4",
    "react-mde": "^7.3.2",
    "react-radio-group": "^3.0.3",
    "react-redux": "^7.0.3",
    "react-responsive-carousel": "^3.1.49",
    "react-stickynode": "^2.1.1",
    "react-tabs": "^3.0.0",
    "react-tooltip": "^3.10.0",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semver": "^6.0.0",
    "striptags": "^3.1.1"
  }
}
