    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^24.8.0",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nock": "^10.0.6",
    "postcss-loader": "^3.0.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.0",
    "tslint": "^5.16.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "aws-sdk": "^2.451.0",
    "bootstrap": "^3.4.1",
    "chroma-js": "^2.0.3",
    "formik": "^1.5.4",
    "humps": "^2.0.1",
    "iframe-phone": "^1.2.0",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "markdown-to-jsx": "^6.9.4",
    "normalizr": "^3.3.0",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-mde": "^7.3.2",
    "react-radio-group": "^3.0.3",
    "react-redux": "^7.0.3",
    "react-responsive-carousel": "^3.1.49",
    "react-stickynode": "^2.1.1",
    "react-tabs": "^3.0.0",
    "react-tooltip": "^3.10.0",
    "react-transition-group": "^4.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "semver": "^6.0.0",
    "striptags": "^3.1.1"
  }
