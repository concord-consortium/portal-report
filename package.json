{
  "name": "@concord-consortium/portal-report",
  "description": "",
  "version": "4.17.0",
  "main": "dist/library/portal-report.js",
  "files": [
    "dist/library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/portal-report.git"
  },
  "keywords": [
    "portal",
    "rigse",
    "report"
  ],
  "author": "Pior Janik <janikpiotrek@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/portal-report/issues"
  },
  "homepage": "https://github.com/concord-consortium/portal-report#readme",
  "scripts": {
    "start": "webpack-dev-server",
    "start:secure": "webpack-dev-server --https --inline --hot --content-base dist/ --cert ~/.localhost-ssl/localhost.pem --key ~/.localhost-ssl/localhost.key",
    "start:secure:no-certs": "webpack-dev-server --https --inline --hot --content-base dist/",
    "build": "webpack --mode production",
    "prepublishOnly": "WEBPACK_TARGET=lib webpack --mode production",
    "lint": "eslint \"js/**/*.{js,ts,tsx}\" \"test/**/*.{js,ts,tsx}\" \"cypress/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --fix \"js/**/*.{js,ts,tsx}\" \"test/**/*.{js,ts,tsx}\" \"cypress/**/*.{js,ts,tsx}\"",
    "cypress:test": "cypress run --config video=false,trashAssetsBeforeRuns=false,defaultCommandTimeout=30000",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:full": "npm run test && npm run cypress:test",
    "doc:diagrams": "mmdc -i docs/diagrams/thirdparty-authed-launch.mmd"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTest.js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "testURL": "https://portal-report.unexisting.url.com",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "testRegex": "(_spec)\\.[tj]sx?$",
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "coverageDirectory": "coverage-jest"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "report-dir": "coverage-cypress"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@mermaid-js/mermaid-cli": "^8.8.4",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^10.0.4",
    "@types/dompurify": "^2.0.4",
    "@types/humps": "^1.1.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/markdown-to-jsx": "^6.11.3",
    "@types/md5": "^2.2.0",
    "@types/react-redux": "^7.1.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "autoprefixer": "^9.7.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "cypress": "^13.6.3",
    "cypress-commands": "^1.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
