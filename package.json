    "dompurify": "^2.0.14",
    "firebase": "^7.14.3",
    "formik": "^2.1.4",
    "html-react-parser": "^0.13.0",
    "humps": "^2.0.1",
    "iframe-phone": "^1.2.1",
    "immutable": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
