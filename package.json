    "file-loader": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^25.5.4",
    "json-loader": "^0.5.7",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.2",
    "source-map-support": "^0.5.19",
    "style-loader": "^1.2.1",
