  "homepage": "https://github.com/concord-consortium/portal-report#readme",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --mode production",
    "prepublishOnly": "WEBPACK_TARGET=lib webpack --mode production",
    "lint": "tslint -c tslint.json 'js/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}' 'cypress/**/*.{js,ts,tsx}'",
    "lint:fix": "tslint -c tslint.json --fix 'js/**/*.{js,ts,tsx}' 'test/**/*.{js,ts,tsx}' 'cypress/**/*.{js,ts,tsx}'",
    "cypress:test": "cypress run --config video=false,trashAssetsBeforeRuns=false",
    "cypress:open": "cypress open",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:full": "npm run test && npm run cypress:test"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTest.js"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "testURL": "https://portal-report.unexisting.url.com",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "testRegex": "(_spec)\\.[tj]sx?$",
    "moduleNameMapper": {
      "\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "autoprefixer": "^9.5.1",
